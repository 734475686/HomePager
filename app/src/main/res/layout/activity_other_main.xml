<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <!--<com.aserbao.homepager.other.MoveFrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>-->
    <com.aserbao.homepager.scroll_view_linearlayout.AScollViewLinearLayout
        android:id="@+id/a_scroll_view"
        android:layout_width="match_parent"
        android:layout_height="60dp">
        <ImageView
            android:src="@mipmap/ic_launcher"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:background="@color/colorAccent"
            android:layout_width="50dp"
            android:layout_height="match_parent"/>
    </com.aserbao.homepager.scroll_view_linearlayout.AScollViewLinearLayout>
    <com.aserbao.homepager.aScollView.TestScollView
        android:id="@+id/test_scoll_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
      <ImageView
                android:src="@drawable/home_background"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
    </com.aserbao.homepager.aScollView.TestScollView>
    <!--<com.aserbao.homepager.BackgroundScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:src="@drawable/home_background"
            android:layout_width="match_parent"
